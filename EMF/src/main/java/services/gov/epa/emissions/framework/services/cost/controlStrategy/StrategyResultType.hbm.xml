<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false"> 
	<class name="gov.epa.emissions.framework.services.cost.controlStrategy.StrategyResultType" table="strategy_result_types" schema="emf">
        <id name="id" type="integer" column="id">
            <generator class="identity"/>
        </id>

        <property name="name" column="name" type="java.lang.String" not-null="true"/>
        <property name="optional" column="optional" type="java.lang.Boolean" not-null="false"/>
        <property name="className" column="classname" type="java.lang.String" not-null="false"/>

    </class>
</hibernate-mapping>  

